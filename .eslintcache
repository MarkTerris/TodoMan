    }\r\n\r\n    return (\r\n        <>\r\n            { !editMode ?\r\n            <ListItem key={id}>\r\n               \r\n                <ListItemIcon>\r\n                    <Checkbox\r\n                        edge=\"start\"\r\n                        color=\"primary\"\r\n                        checked={completed}\r\n                        onChange={() => dispatch(completeTodo(id))}\r\n                        disableRipple\r\n                    />\r\n                </ListItemIcon>\r\n\r\n                <ListItemText \r\n                    primary={value} \r\n                    secondary={date.slice(11, 16) + \"  \" + date.slice(5, 7) + \"/\" + date.slice(8, 10)} \r\n                    className={completed ? classes.textItem + classes.completedItem : classes.textItem}\r\n                /> \r\n\r\n                <ListItemSecondaryAction>\r\n                    {!completed && \r\n                    <IconButton onClick={() => setEditMode(!editMode)} edge=\"end\" className={classes.secondaryButton}>\r\n                        <Edit />\r\n                    </IconButton>}\r\n                    \r\n                    <IconButton onClick={() => dispatch(deleteTodo(id))} \r\n                        edge=\"end\" \r\n                        color=\"primary\">\r\n                        <Delete />\r\n                    </IconButton>\r\n                </ListItemSecondaryAction>\r\n            </ListItem> :\r\n            <ListItem key={id}>\r\n\r\n            <ListItemIcon className={classes.cancelEdit}>\r\n                <IconButton onClick={() => setEditMode(!editMode)}>\r\n                    <Close\r\n                        edge=\"start\"\r\n                        color=\"default\"\r\n                    />\r\n                </IconButton>\r\n            </ListItemIcon> \r\n\r\n            <form onSubmit={handleEditItem}>\r\n                <ListItemText className={classes.editTextItem}>\r\n                    <TextField\r\n                        value={newItemToAdd}\r\n                        onChange={(e) => setNewItem(e.target.value)}\r\n                        type=\"text\"\r\n                    />\r\n                </ListItemText>\r\n\r\n                <ListItemSecondaryAction>\r\n                    <IconButton type=\"submit\" edge=\"end\" color=\"primary\" disabled={newItemToAdd.length === 0 || newItemToAdd === value}>\r\n                        <Done />\r\n                    </IconButton>\r\n                </ListItemSecondaryAction>\r\n            </form>\r\n        </ListItem> }\r\n    </>\r\n    )\r\n}","C:\\Users\\Mark\\todoman\\app\\src\\components\\Todos\\TodoLists.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Todos\\TodoListItem.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Home\\NavBar.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Home\\HomePage.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Home\\HomePageAuthed.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Home\\Footer.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Home\\TodoPage.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Auth\\LoginPage.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\components\\Auth\\RegisterPage.jsx",[],"C:\\Users\\Mark\\todoman\\app\\src\\store.js",[],"C:\\Users\\Mark\\todoman\\app\\src\\reducers\\index.js",[],"C:\\Users\\Mark\\todoman\\app\\src\\actions\\types.js",[],"C:\\Users\\Mark\\todoman\\app\\src\\reducers\\todosReducer.js",[],"C:\\Users\\Mark\\todoman\\app\\src\\actions\\todosActions.js",[],"C:\\Users\\Mark\\todoman\\app\\src\\actions\\listsActions.js",[],"C:\\Users\\Mark\\todoman\\app\\src\\reducers\\listsReducer.js",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":4,"column":33,"nodeType":"98","messageId":"99","endLine":4,"endColumn":43},{"ruleId":"96","severity":1,"message":"100","line":14,"column":36,"nodeType":"98","messageId":"99","endLine":14,"endColumn":51},